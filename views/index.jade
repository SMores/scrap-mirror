extends layout

block content
  - var space = {id: 1, columnSorting: [4,3,1,2], columns: [{id: 4, elementSorting: [2,1], SpaceId: 1, elements: [{id: 2, contentType: 'text', content: 'Aaand here\'s the second'}, {id: 2, contentType: 'text', content: 'This is the first element'}]}, {id: 1, elementSorting: [3], SpaceId: 1, elements: [{id: 3, contentType: 'text', content: "This is the third"}]}, {id: 2, elementSorting: [4], SpaceId: 1, elements: [{id: 3, contentType: 'text', content: "Fourth woo!"}]}]}
  - var users = ["https://fbcdn-profile-a.akamaihd.net/hprofile-ak-xap1/v/t1.0-1/c1.95.477.477/p480x480/1920336_10152275444250993_2135489901_n.jpg?oh=bbaa145748095d6331cba2e63d34351e&oe=544B7BC0&__gda__=1412714693_09ab5f6cad5ec3a6e1b1a5cfbdf15e02","https://fbcdn-profile-a.akamaihd.net/hprofile-ak-xap1/v/t1.0-1/c0.0.480.480/p480x480/10176080_2520381682312_4890391499590307710_n.jpg?oh=9b62cd1a1e536d8baeb80f5b63b12aee&oe=544C61A6&__gda__=1414538666_07bda3f9b428206ce7dab047fcd01710"]
  
  header
    ul.users
      each user in users
        li
          img(src= user)

  section.content
    each column in space.columns
      section.column
        each element in column.elements
          if element.contentType == 'text'
            article.element
              p= element.content